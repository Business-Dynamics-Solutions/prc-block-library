{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "prc-block/form-input-password",
	"version": "1.0.0",
	"title": "Password Field",
	"category": "design",
	"description": "A block for password input in a form with optional confirmation field and analyzer.",
	"attributes": {
		"includesConfirmation": {
			"type": "boolean",
			"default": false
		},
		"style": {
			"type": "object",
			"default": {
				"spacing": {
					"blockGap": "var:preset|spacing|10"
				},
				"typography": {
					"lineHeight": "1"
				}
			}
		}
	},
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": false,
		"__experimentalBorder": {
			"color": true,
			"width": true
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true
		},
		"interactivity": true
	},
	"textdomain": "form-input-password",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js"
}
